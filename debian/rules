#!/usr/bin/make -f
export DH_VERBOSE = 1
%:
	dh $@  --with autoreconf

override_dh_auto_clean:
	[ ! -f Makefile ] || $(MAKE) distclean

override_dh_installdocs:
	dh_installdocs NEWS

override_dh_builddeb:
	dh_builddeb -- -Zzstd

override_dh_auto_install:
	$(MAKE) DESTDIR=$$(pwd)/debian/hsencfs prefix=/usr install

# EOF

